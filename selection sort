import java.util.*;
class Main {
    public static void main(String[] args) {
        int[] arr = {64,78,56,85,24,7};
        for(int i=0;i<arr.length-1; i++)
        {
            int min=i;
            for(int  j=i+1;j<arr.length;j++){
                if(arr[j] < arr[min])
                min=j;
            }
            int temp = arr[min];
            arr[min]=arr[i];
            arr[i]= temp;
        }
        System.out.println("Selecetion sorted : "+Arrays.toString(arr));
    }
}
