class Node{
    int data;
    Node front;
    Node rear;
    Node next;
    
    Node(int value){
        this.data=value;
        this.front=null;
        this.rear=null;
        this.next=null;
    }
}
class queue{
    Node front;
    Node rear;
    public void enqueue(int value){
        Node newnode = new Node(value);
        if(rear==null){
            
            front =newnode;
            rear=newnode;
            return;
        }
        
        rear.next=newnode;
        rear=newnode;
}
public void dequeue(){
    if(front==null){
        System.out.println("empty");
    }
    int val=front.data;
    front =front.next;
    if(front ==null)rear=null;
    //System.out.println(val);
    
}
public void display(){
    if(front ==null){
        System.out.println("list is empty");
    }
    Node temp=front;
    while(temp!=null){
        System.out.println(temp.data+"");
        temp=temp.next;
    }
}
}
public class Main{
    public static void main(String[]args){
        queue q= new queue();
        q.enqueue(3);
        q.enqueue(8);
        q.enqueue(4);
        q.display();
        q.dequeue();
        q.display();
    }
}
